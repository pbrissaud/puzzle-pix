{
  "name": "puzzle-pix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "ui:add": "turbo run ui:add --filter=@repo/ui --",
    "db:generate": "turbo run db:generate --filter=@repo/db",
    "db:push": "turbo run db:push --filter=@repo/db"
  },
  "dependencies": {},
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "prettier": "^3.2.5",
    "turbo": "^2.0.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@9.4.0"
}
